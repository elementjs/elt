{
  "name": "elt",
  "version": "1.0.0-alpha12",
  "repository": "https://github.com/elementjs/elt",
  "description": "",
  "license": "MIT",
  "browser": "./dist/elt.js",
  "main": "./dist/elt.js",
  "jsnext:main": "./module/index.js",
  "typings": "./dist/elt.d.ts",
  "scripts": {
    "build:dts": "dts-bundle --name elt --main module/index.d.ts --out ../dist/elt.d.ts && sed -i 's/^global/declare global/' ./module/elt.d.ts",
    "build:js": "tsc ; rollup -c",
    "build": "npm run build:js && npm run build:dts",
    "prepublish": "npm run build",
    "watch": "concurrently -c green,blue,red -n dtsbundle,tsc,rollup 'onchange \"./module/*.js\" -- npm run build:dts' 'tscc -w' 'rollup -c -w'"
  },
  "devDependencies": {
    "concurrently": "^3.5.0",
    "convert-source-map": "^1.5.0",
    "dts-bundle": "^0.7.3",
    "onchange": "^3.2.1",
    "rollup-plugin-node-resolve": "^3.0.0",
    "rollup-plugin-sourcemaps": "^0.4.2",
    "tslib": "^1.8.0"
  },
  "dependencies": {}
}
